package acmicpc.net;
import java.util.Arrays;
import java.util.Comparator;
import java.util.Scanner;

public class Number_1931 {

	public static void main(String[] args) {
		
		class Time{
			int start;
			int end;
			Time(int start, int end){
				this.start = start;
				this.end = end;
			}
		}
		
//		Scanner scanner = new Scanner(System.in);
//		
//		int n = scanner.nextInt();
//    	Time[] times = new Time[n];
//		for(int i =0; i < n; i++) {
//			times[i] = new Time(scanner.nextInt(), scanner.nextInt());
//		}
    	
		Time[] times = new Time[3];
		times[0] = new Time(1, 1);
		times[1] = new Time(1, 1);
		times[2] = new Time(1, 1);
		
		Arrays.sort(times, new Comparator<Time>() {
			public int compare(Time o1, Time o2) {
				return o1.end-o2.end;
			};
		});
		
		int cur = times[0].end;
		int cnt =1;
		
		for(int i =1; i<n; i++) {
			if(cur <= times[i].start) {
				cnt++;
				cur = times[i].end;
			}
		}
		
		System.out.println();	
	}

}
