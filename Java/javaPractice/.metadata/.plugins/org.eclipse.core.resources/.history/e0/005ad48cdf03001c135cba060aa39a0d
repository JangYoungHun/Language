package Programmers;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

public class Number_42890 {
	
	static List<String> resultList = new ArrayList<String>();
	public static void main(String[] args) {
		
		String relation[][] = {{"100","ryan","music","2"},{"200","apeach","math","2"},{"300","tube","computer","3"},{"400","con","computer","4"},{"500","muzi","music","3"},{"600","apeach","music","2"}};
		System.out.println(solution(relation));
	}
	
	   public static int solution(String[][] relation) {
		   
		   int n = relation.length;
		   int m = relation[0].length;
		   
		   for(int i =1; i<=m; i++) {
			   dfs(relation, new ArrayList<Integer>(), n,  m, 0, i);
		   }
		    int answer =0;
	        return answer;
	    }
	   
	   static void dfs(String[][] relation, List<Integer> list, int n, int m, int start, int cnt) {
		   
		   if(list.size() == cnt) {
			   StringBuilder sb =new StringBuilder();
			   String data[] = new String[n];
			   for(int i =0; i<list.size(); i++) {
				   sb.append(list.get(i));
				   for(int k=0; k<n; k++) {
					   data[k] += relation[k][list.get(i)];
				   }
			   }
			   
			   Set<String> set = new HashSet<String>(Arrays.asList(data));
			   
			   if(set.size() == data.length) {
				   
				   if(resultList.size() ==0) {
					   resultList.add(sb.toString());
					   return;
				   }
				   else {
					   for(int i =0; i<resultList.size(); i++) {
						   if(resultList.get(i).contains(sb.toString()))
							   return;
					   }
				   }
				   resultList.add(sb.toString());
				   return;
			   }
		   }
		   
		   for(int i =start; i<m; i++) {
			   if(!list.contains(start)) {
				   list.add(i);
				   dfs(relation ,list, n, m, i, cnt);
				   list.remove(list.size()-1);
			   }
		   }
		   
	   }
	   
}
