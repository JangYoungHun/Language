package Array;

public class Move_Zeros {

	public static void main(String[] args) {
		// 0을 오른쪽으로 이동시키기
		int arr[] = {0,5,0,7,6,3};
		
		int i=0;
		for(int j=0; j<arr.length; j++) {
			if(arr[j] != 0) {
				swap(arr, i, j);
			}
		}
		
		
	}
	
	
	static void swap(int arr[], int num1, int num2) {
		int temp = arr[num1];
		arr[num1] = arr[num2];
		arr[num2] = temp;
	}

}
